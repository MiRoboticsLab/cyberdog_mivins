<launch>

  <arg name="type" default="vo"/>

  <arg name="calib_file" default="$(find svo_ros)/param/calib/dog/mi_fisheye_stereo.yaml"/>
  <arg name="config_file" default="$(find svo_ros)/param/mi_$(arg type)_fisheye_stereo.yaml"/>

  <node pkg="svo_ros" type="svo_node" name="svo" clear_params="true" output="screen" args="--v=0">

    <param name="cam0_topic" value="/image_left" type="str" />
    <param name="cam1_topic" value="/image_right" type="str" />
    <!-- param name="imu_topic" value="/mi0017090/camera/imu" type="str" /> -->

    <param name="calib_file" value="$(arg calib_file)" />
    <param name="config_file" value="$(arg config_file)" />

  </node>


  <!--<node name="vis" pkg="rviz" type="rviz" args=" -d $(find svo_ros)/rviz_config_vio.rviz" />-->

</launch>
