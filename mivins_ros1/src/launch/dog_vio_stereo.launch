<!-- Launch File for running svo with ceres backend from bag-file -->
<launch>

  <arg name="type" default="vio"/>

  <!-- For realsense camera -->
  <!-- <arg name="calib_file" default="$(find svo_ros)/param/calib/dog/dog_stereo_rs_pinhole_none.yaml"/> -->

  <!-- For fisheye camera -->
  <!-- <arg name="calib_file" default="$(find svo_ros)/param/calib/dog/dog_stereo_fisheye_pinhole_equi.yaml"/> -->
  <arg name="calib_file" default="$(find svo_ros)/param/calib/dog/dog_stereo_fisheye_mei.yaml"/>

  <arg name="config" default="$(find svo_ros)/param/dog_$(arg type)_stereo_fisheye.yaml"/>


  <!-- SVO node -->
  <node pkg="svo_ros" type="svo_node" name="svo" clear_params="true" output="screen" args="--v=0">

    <!-- Camera topic to subscribe to -->
    <!-- For realsense camera -->
    <!-- <param name="cam0_topic" value="/camera/infra1/image_rect_raw" type="str" />
    <param name="cam1_topic" value="/camera/infra2/image_rect_raw" type="str" /> -->
	
    <!-- For fisheye camera -->
    <param name="cam0_topic" value="/image_left" type="str" />
    <param name="cam1_topic" value="/image_right" type="str" />
	
    <!-- Imu topic to subscribe to -->
    <param name="imu_topic" value="/camera/imu" type="str" />

    <!-- Camera calibration file -->
    <param name="calib_file" value="$(arg calib_file)" />

    <!-- Config file -->
    <param name="config_file" value="$(arg config_file)" />

    <!--Parameters-->
    <!-- For realsense camera -->
    <!-- <rosparam file="$(find svo_ros)/param/dog_vio_stereo_realsense.yaml" /> -->

    <!-- For fisheye camera -->
    <param name='runlc' value='false' />

  </node>

  <!-- RVIZ -->
  <!-- <node name="vis" pkg="rviz" type="rviz" args=" -d $(find svo_ros)/rviz_config_vio.rviz" /> -->

</launch>
